package Asterisk::LCR::Dialer::MinTime;
use base qw /Asterisk::LCR::Dialer/;
use warnings;
use strict;


sub _process
{
    my $self  = shift;
    my $num   = shift || return;
    my @rates = $self->rates ($num);
    
    @rates > 0 or return;
    
    my $str = join '&', map { $self->dial_string ($num, $_) } @rates;
    return [ $str ];
}


1;


__END__
